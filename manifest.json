{
	"manifest_version": 2,

	"name": "Draugiem.lv profila jaunumi",
	"description": "Paplašinājums paziņojumu veidā attēlo jaunumus lietotāja Draugiem.lv profilā.",
	"homepage_url": "https://github.com/arvislacis/draugiemlv_pj",
	"version": "0.2.5",

	"icons": {
		"16": "img/ikona16.png",
		"48": "img/ikona48.png",
		"128": "img/ikona128.png"
	},

	"minimum_chrome_version": "23",

	"permissions": [
		"http://www.draugiem.lv/",
		"alarms",
		"notifications",
		"storage"
	],

	"background": {
		"scripts": ["js/jquery.min.js", "js/skripts.js"],
		"persistent": false
	},

	"options_page": "html/iestatijumi.html"
}